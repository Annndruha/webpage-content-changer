{
  "name": "Content changer",
  "manifest_version": 3,
  "version": "0.5",
  "description": "Apply rule for site.",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "run_at": "document_end",
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_title": "Content changer",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
