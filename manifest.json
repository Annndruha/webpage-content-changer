{
  "name": "Content changer",
  "manifest_version": 3,
  "version": "0.3",
  "description": "Apply rule for site.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Content changer",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"]
}
